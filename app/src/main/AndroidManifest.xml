<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ganiko.vitor.spajulioganiko">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:name="fernandosousa.com.br.lmsapp.LMSApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity"
                android:theme="@style/AppTheme">    >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".DebugActivity" />

        <activity
            android:name=".TelaInicialActivity"
            android:theme="@style/AppTheme.Toolbar" />


        <activity
            android:name=".TelaClienteActivity"
            android:theme="@style/AppTheme.Toolbar" />




        <activity
            android:name=".TelaTerpiaActivity"
            android:theme="@style/AppTheme.Toolbar" />

        <activity
            android:name=".TelaConsultaActivity"
            android:theme="@style/AppTheme.Toolbar" />


        <activity
            android:name=".TelaConsultaActivityTeste"
            android:theme="@style/AppTheme.Toolbar" />


        <activity
            android:name=".TelaSobreActivity"
            android:theme="@style/AppTheme.Toolbar" />


        <activity
            android:name=".TelaCadastroActivity"
            android:theme="@style/AppTheme.Toolbar" />




        <activity
            android:name=".TelaCadastroTerapiaActivity"
            android:theme="@style/AppTheme.Toolbar" />


        <activity
            android:name=".TelaCalendario"
            android:theme="@style/AppTheme.Toolbar" />






            <activity
                android:name=".ClienteActivity"
                android:parentActivityName=".TelaClienteActivity"
                android:theme="@style/AppTheme.Toolbar" />



            <activity
                android:name=".TelaCadastroConsultaActivity"
                android:theme="@style/AppTheme.Toolbar" />





            <activity
                android:name=".TerapiaActivity"
                android:parentActivityName=".TelaTerpiaActivity"
                android:theme="@style/AppTheme.Toolbar" />







            <activity
                android:name=".ConsultaActivity"
                android:parentActivityName=".TelaConsultaActivityTeste"
                android:theme="@style/AppTheme.Toolbar" />



        <service android:name=".MyFirebaseMessagingService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>


    </application>

</manifest>